<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bienvenida"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9F9F9"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/icono_app_bienvenida"
        android:layout_width="152dp"
        android:layout_height="203dp"
        android:layout_marginStart="128dp"
        android:layout_marginLeft="128dp"
        android:layout_marginTop="180dp"
        android:background="#192858"
        android:contentDescription="@string/icono_app_bienvenida"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/uno" />

    <Button
        android:id="@+id/btn_ellori"
        android:layout_width="147dp"
        android:layout_height="68dp"
        android:layout_marginStart="132dp"
        android:layout_marginLeft="132dp"
        android:layout_marginTop="116dp"
        android:background="#192858"
        android:onClick="a_login"
        android:text="@string/btn_ellori"
        android:textColor="#E2E9EC"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icono_app_bienvenida" />
</androidx.constraintlayout.widget.ConstraintLayout>